{
  "extends": [
    "airbnb",
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "no-unused-vars": [
      1,
      {
        "vars": "all", "args": "after-used", "ignoreRestSiblings": false
      }
    ],
    "no-console": 0
  }
 }