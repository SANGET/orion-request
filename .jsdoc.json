{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": "./src",
    "excludePattern": "./src/libs"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "systemName": "Uke-Request",
    "systemSummary": "基于 fetch 封装的请求库",
    "systemColor": "#449bea",
    "copyright": "Uke-Request",
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./jsdocs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}