{
  "env": {
    "test": {
      "presets": ["@babel/preset-env"]
    },
    "development": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        [
          "react-hot-loader/babel"
        ],
        ["@babel/plugin-proposal-class-properties"]
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
