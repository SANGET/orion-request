{
  "name": "orion-request",
  "version": "2.1.11",
  "description": "",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js",
    "push": "npm publish; git add -A; git commit -m 'update'; git push origin master"
  },
  "peerDependencies": {
    "basic-helper": "^1.3.x"
  },
  "repository": {
    "type": "git",
    "url": "http://github.9th.go/matrix-front-end/orion-request.git"
  },
  "author": "alex",
  "license": "ISC",
  "babel": {
    "env": {
      "production": {
        "presets": [
          "env"
        ]
      }
    }
  }
}
