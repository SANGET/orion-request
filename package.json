{
  "name": "uke-request",
  "version": "0.11.2",
  "description": "通讯加密，消息压缩，RESTFul",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js"
  },
  "peerDependencies": {
    "basic-helper": "^1.3.x",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "fs-extra": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.9th.go/matrix-front-end/uke-request.git"
  },
  "author": "alex",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  }
}
