{
  "name": "uke-request",
  "version": "1.0.0",
  "description": "通讯加密，消息压缩，RESTFul",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm version patch; npm run publish:build",
    "publish:build": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js",
    "jsdoc": "jsdoc -c .jsdoc.json ./src",
    "docz:dev": "docz dev",
    "docz:build": "NODE_ENV=production docz build",
    "test": "jest"
  },
  "peerDependencies": {
    "basic-helper": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "basic-helper": "^3.0.3",
    "docz": "^1.2.0",
    "docz-plugin-css": "^0.11.0",
    "docz-plugin-netlify": "^1.1.4",
    "docz-theme-default": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "foodoc": "^0.0.9",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "ts-jest": "^24.0.2",
    "typescript": "3.4.5",
    "ukelli-ui": "^2.17.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SANGET/uke-request.git"
  },
  "author": "Alex",
  "license": "MIT",
  "dependencies": {
    "lzma": "^2.3.2",
    "rc4-ts": "^2.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
